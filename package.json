{
  "author": "Arthur Fiorette",
  "name": "typed-core",
  "version": "1.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:types": "tsc --project tsconfig.types.json",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts",
    "prettify": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurFiorette/typed-core.ts.git"
  },
  "keywords": [
    "typed-core",
    "typescript",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/ArthurFiorette/typed-core/issues"
  },
  "homepage": "https://github.com/ArthurFiorette/typed-core#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.8"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.5",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "prettier-plugin-jsdoc": "^0.3.24",
    "prettier-plugin-organize-imports": "^2.3.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.4.2"
  }
}
